{
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/rpc",
      "dest": "/api/app"
    },
    {
      "src": "/schema", 
      "dest": "/api/app"
    },
    {
      "src": "/health",
      "dest": "/api/app"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
